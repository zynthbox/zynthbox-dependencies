Index: jalv/wscript
===================================================================
--- jalv.orig/wscript
+++ jalv/wscript
@@ -235,7 +235,7 @@
                   lib          = ['pthread'],
                   uselib       = libs + ' QT5',
                   install_path = '${BINDIR}',
-                  cxxflags     = ['-fPIC'])
+                  cxxflags     = ['-fPIC', '-std=c++11'])
 
     # Man pages
     bld.install_files('${MANDIR}/man1', bld.path.ant_glob('doc/*.1'))
